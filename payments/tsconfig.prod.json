{
    "compilerOptions": {
        "target": "es2016", 
        "module": "commonjs",
        "baseUrl": "./src", 
        "outDir": "./build", 
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true, 
        "skipLibCheck": true,
        "noImplicitAny": false,
    },
    "exclude": [
      "node_modules",
      "src/**/*.test.ts",
      "src/test/setup.ts",
      "src/__mocks__"
    ],
  }